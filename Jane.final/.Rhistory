mix.nor.dis <- function(n, p, mu1, mu2, sd1, sd2){
y1 <- rnorm(n,mean=mu1, sd = sd1)
y0 <- rnorm(n,mean=mu2, sd = sd2)
flag <- LaplacesDemon::rbern(n,prob=p)
y <- y1*flag + y0*(1-flag)
return(y)
}
